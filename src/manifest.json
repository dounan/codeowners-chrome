{
  "name": "CODEOWNERS PR Filter",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://github.com/*"],
      "css": [],
      "js": ["content.bundle.js"]
    }
  ],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "permissions": ["cookies", "webNavigation", "*://github.com/*"]
}
